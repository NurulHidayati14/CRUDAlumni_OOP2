<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.crudalumni">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.crudalumni.UserAdapter"
            tools:ignore="Instantiatable" />
        <activity android:name="com.example.crudalumni.SiswaAdapter"
            tools:ignore="Instantiatable" />
        <activity android:name="com.example.crudalumni.EditUserActivity" />
        <activity android:name="com.example.crudalumni.UserActivity" />
        <activity android:name="com.example.crudalumni.EditSiswaActivity" />
        <activity android:name="com.example.crudalumni.Siswactivity" />
        <activity android:name="com.example.crudalumni.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>